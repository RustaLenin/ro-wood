<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RecipeDef>
        <defName>HSK_MakeWoodComponent_Hand</defName>
        <label>make wood component (x4)</label>
        <description>Carve simple wood component out of timber log.</description>
        <jobString>Carving wood components.</jobString>
        <effectWorking>MakeWoodPlanks_Hand</effectWorking>
        <soundWorking>HSK_WoodWorking</soundWorking>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>HSK_RawTimber</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>HSK_RawTimber</li>
            </categories>
        </fixedIngredientFilter>
        <products>
            <HSK_ComponentWood>4</HSK_ComponentWood>
            <HSK_Wooddust>200</HSK_Wooddust>
            <HSK_SlabPlank>4</HSK_SlabPlank>
        </products>
        <workSkill>Crafting</workSkill>
        <workSkillLearnFactor>0.25</workSkillLearnFactor>
        <workAmount>500</workAmount>
        <workSpeedStat>HSK_WoodWorkingSpeed</workSpeedStat>
        <unfinishedThingDef>HSK_UnfinishedWoodWork</unfinishedThingDef>
        <recipeUsers>
            <li>HSK_WoodWorkingSpot</li>
            <li>HSK_Sawmill</li>
        </recipeUsers>
    </RecipeDef>

