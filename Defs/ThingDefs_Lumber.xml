<Defs>

    <!-- WoodSlab Planks-->
    <ThingDef Name="SlabPlank" ParentName="TimberBase">
        <defName>SlabPlank</defName>
        <label>slabplank</label>
        <description>Cheap and low quality wood plank with tree bark on from one of the sides</description>
        <graphicData>
            <texPath>LumberSlab</texPath>
        </graphicData>
        <statBases>
            <MaxHitPoints>30</MaxHitPoints>
            <Beauty>-1</Beauty>
            <MarketValue>0.3</MarketValue>
            <Mass>0.8</Mass>
        </statBases>
        <stackLimit>500</stackLimit>
        <stuffProps>
            <statOffsets>
                <Beauty>-1</Beauty>
            </statOffsets>
            <appearance>Planks</appearance>
            <stuffAdjective>Slabplank</stuffAdjective>
            <categories Inherit="False">
                <li>Woody</li>
                <li>WoodSlabs</li>
            </categories>
            <statFactors>
                <MaxHitPoints>0.3</MaxHitPoints>
                <Flammability>1.5</Flammability>
                <Beauty>0.3</Beauty>
                <WorkToMake>0.30</WorkToMake>
                <WorkToBuild>0.30</WorkToBuild>
                <MeleeWeapon_CooldownMultiplier>1.5</MeleeWeapon_CooldownMultiplier>
                <MoveSpeed>0.8</MoveSpeed>
                <DoorOpenSpeed>0.8</DoorOpenSpeed>
                <MarketValue>0.2</MarketValue>
                <BedRestEffectiveness>0.5</BedRestEffectiveness>
                <ImmunityGainSpeedFactor>0.8</ImmunityGainSpeedFactor>
                <WorkTableWorkSpeedFactor>0.3</WorkTableWorkSpeedFactor>
                <Mass>2</Mass>
            </statFactors>
        </stuffProps>
        <tools>
            <li>
                <label>log</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>5</power>
                <cooldownTime>1</cooldownTime>
            </li>
        </tools>
        <thingCategories Inherit="False">
            <li>WoodSlabs</li>
        </thingCategories>
    </ThingDef>

