    <RecipeDef>
        <defName>ROWood_SawCecropia</defName>
        <label>cecropia plank(x20)</label>
        <description>Saw cecropia timber logs into planks.</description>
        <jobString>Sawing cecropia logs.</jobString>
        <effectWorking>MakeWoodPlanks_Hand</effectWorking>
        <soundWorking>SawLog</soundWorking>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>CecropiaLog</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <products>
            <CecropiaPlank>20</CecropiaPlank>
            <SlabPlank>4</SlabPlank>
        </products>
        <workSkill>Crafting</workSkill>
        <workSkillLearnFactor>0.35</workSkillLearnFactor>
        <workAmount>600</workAmount>
        <workSpeedStat>ROWood_WoodWorkingSpeed</workSpeedStat>
        <unfinishedThingDef>UnfinishedWoodWork</unfinishedThingDef>
    </RecipeDef>

