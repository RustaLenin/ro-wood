<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <SoundDef>
        <defName>LogChopping</defName>
        <context>MapOnly</context>
        <sustain>True</sustain>
        <priorityMode>PrioritizeNearest</priorityMode> 
        <subSounds>
            <li>
                <onCamera>True</onCamera>
                <muteWhenPaused>True</muteWhenPaused> 
                <grains>
                    <li Class="AudioGrain_Folder">
                        <clipFolderPath>ChopLog</clipFolderPath>
                    </li>
                </grains>
                <volumeRange>
                    <min>35</min>
                    <max>45</max>
                </volumeRange>
                <!-- <pitchRange>0.2~5</pitchRange> -->
                <sustainLoop>False</sustainLoop>
            </li>
        </subSounds>
    </SoundDef>

    <SoundDef>
        <defName>SawLog</defName>
        <context>MapOnly</context>
        <sustain>True</sustain>
        <priorityMode>PrioritizeNearest</priorityMode> 
        <subSounds>
            <li>
                <onCamera>True</onCamera>
                <muteWhenPaused>True</muteWhenPaused> 
                <grains>
                    <li Class="AudioGrain_Folder">
                        <clipFolderPath>SawingWood</clipFolderPath>
                    </li>
                </grains>
                <volumeRange>
                    <min>35</min>
                    <max>45</max>
                </volumeRange>
                <!-- <pitchRange>0.2~5</pitchRange> -->
                <sustainLoop>False</sustainLoop>
            </li>
        </subSounds>
    </SoundDef>

</Defs>